{
  "name": "react-minimal-boilerplate",
  "version": "1.0.0",
  "description": "This is a minimal boilerplate for React development",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack/webpack.config.js --open --env=dev",
    "build": "webpack --config ./webpack/webpack.config.js --env=prod",
    "deploy:gh-pages": "rimraf dist && npm run build && rimraf dist/report.html && gh-pages -d dist",
    "lint": "eslint .",
    "format": "pretty-quick",
    "test": "jest",
    "start": "node .",
    "heroku-postbuild": "rimraf dist && npm run build && rimraf dist/report.html"
  },
  "author": "tabomors",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@react-mock/fetch": "^0.3.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-fetch": "^2.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "react-testing-library": "^7.0.1",
    "rimraf": "^2.6.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1",
    "querystringify": ">=2.0.0"
  }
}
